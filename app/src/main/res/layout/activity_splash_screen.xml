<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".SplashScreen">

	<ImageView
		android:id="@+id/load_Picup"
		android:layout_width="354dp"
		android:layout_height="114dp"
		android:layout_marginTop="-5dp"
		android:contentDescription="@string/loadpic"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/ic_pic_up" />

	<ImageView
		android:id="@+id/load_Picdown"
		android:layout_width="368dp"
		android:layout_height="wrap_content"
		android:contentDescription="@string/loadpic"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:srcCompat="@drawable/ic_pic_down" />

	<ImageView
		android:id="@+id/load_txtBazimoon"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:src="@drawable/bazimoon_logo"
		app:layout_constraintBottom_toTopOf="@+id/load_Picdown"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/load_Picup"
		/>

</androidx.constraintlayout.widget.ConstraintLayout>